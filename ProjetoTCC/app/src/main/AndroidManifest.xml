<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetotcc">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:windowSoftInputMode="adjustResize|stateAlwaysHidden"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChatUsuario" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InfoServico"/>
        </activity>
        <activity android:name=".InfoServico" />
        <activity android:name=".cadastroServico.CadastroServico1" />
        <activity android:name=".cadastroUsuario.Cadastro5" />
        <activity android:name=".cadastroUsuario.Cadastro4" />
        <activity android:name=".cadastroUsuario.Cadastro3" />
        <activity android:name=".cadastroUsuario.Cadastro2" />
        <activity android:name=".cadastroUsuario.Cadastro1" />
        <activity
            android:name=".PaginaUsuario"
            android:label="@string/title_activity_pagina_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>